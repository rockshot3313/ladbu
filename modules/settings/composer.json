{
    "name": "ladbu/laravel-ladwire-settings",
    "description": "Settings module for Laravel Ladwire Module package",
    "type": "laravel-package",
    "keywords": ["laravel", "ladwire", "settings", "module"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "livewire/livewire": "^2.0|^3.0",
        "fluxui/flux": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ladbu\\LaravelLadwireSettings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ladbu\\LaravelLadwireSettings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladbu\\LaravelLadwireSettings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
